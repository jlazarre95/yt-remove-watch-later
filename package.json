{
  "name": "yt-remove-watch-later",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "ts-node dist/src/index.js",
    "start:debug": "ts-node --inspect-brk dist/src/index.js",
    "test": "mocha --config .mocharc.yaml",
    "test:cov": "nyc mocha --config .mocharc.yaml",
    "test:debug": "mocha --inspect-brk --config .mocharc.yaml",
    "test:int": "mocha --config .mocharc.int.yaml",
    "test:int:cov": "nyc mocha --config .mocharc.int.yaml",
    "test:int:debug": "mocha --inspect-brk --config .mocharc.int.yaml",
    "docker:build": "docker build -t yt-remove-watch-later .",
    "docker:run": "docker run --env-file=.env --rm -p 8081:8081 --name yt-remove-watch-later yt-remove-watch-later",
    "docker:run:watch": "tsc-watch --onSuccess \"npm run docker:buildAndRun\"",
    "docker:buildAndRun": "npm run docker:build && npm run docker:run",
    "docker:stop": "docker rm -f yt-remove-watch-later"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testdeck/mocha": "^0.3.2",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.10",
    "chai": "^4.3.7",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "boolean": "^3.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "fs-extra": "^11.1.0",
    "ghost-cursor": "^1.1.16",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.39",
    "puppeteer": "^19.3.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "reflect-metadata": "^0.1.13"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "require": [
      "ts-node/register/transpile-only"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.test.ts",
      "coverage",
      "bin"
    ],
    "reporter": [
      "text",
      "html"
    ]
  }
}
